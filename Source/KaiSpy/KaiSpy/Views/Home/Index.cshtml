@{ Layout = null;}

<html>
<head>
    <title>KaiSpy</title>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/App")
    @Styles.Render("~/Content/css")
    <link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Fjalla+One' rel='stylesheet' type='text/css'>
</head>
<body>
<h1 id ="Headaa">Kai-Spy</h1>

    <aside id="pref">
        <h3>Preferences:</h3>

        <form id="foodtype">

        </form>
        
        <input id="search-radius-slider" type="range" min="1" max="10" value="3" oninput="SetCircleRadiusBySlider(this.value)"/>
        <span id="search-radius">3 km</span>
        <div id="tester">
            <button id="remove-all-pins" type="button">Clear All</button>
            <button id="add-all-pins" type="button">Show All</button>
        </div>
    </aside>

    <section id='map'>
        <div id='map-canvas'></div> 
    </section>

    <section id="details">
        <h2>Detail here</h2>
    </section>

    </body>
</html>